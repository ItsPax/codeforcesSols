# take the absolute difference between i and 3
# take the absolute difference between j and 3
# return the sum of these two numbers

def beautifulMatrix():
	for i in range(1,6):
		colList = [x for x in input().split()]
		for j in range(1,6):
			if colList[j-1] == '1':
				iCoord = i
				jCoord = j
	return abs(iCoord-3) + abs(jCoord-3)

print(beautifulMatrix())
