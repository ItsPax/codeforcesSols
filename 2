def doubleCola(n):
	trueVal = 0
	subVal = 5
	exp = 1
	
	while n > 0:
		trueVal = n 
		n -= subVal
		subVal = 5*2**exp
		exp += 1
	exp -= 1
	print(trueVal)
	character = 0
	print(2**exp)
	while trueVal > 0:
		trueVal -= 2**exp
		character += 1
	trueVal += 2**exp
	character -= 1
	charDict = {0:'Sheldon',1:'Leonard',2:'Penny',3:'Rajesh',4:'Howard'}
	return charDict[character]
	
print(doubleCola(int(input())))
